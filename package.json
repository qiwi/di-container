{
  "name": "di-container",
  "version": "1.0.0",
  "description": "Experiments around DI and IoC",
  "main": "index.js",
  "@std/esm": {
    "esm": "all",
    "cjs": true
  },
  "scripts": {
    "flow": "flow",
    "flow_coverage": "flow-coverage-report -i 'src/**/*.js' -t html -t json -t text --threshold 90",
    "build": "babel src --out-dir dist",
    "lint": "standard src/**/*.js test/**/*.js",
    "mocha": "nyc mocha --opts .mocha.opts",
    "test": "npm run flow_coverage && npm run lint && npm run mocha",
    "doc": "esdoc"
  },
  "repository": {
    "type": "git",
    "url": "git@github.qiwi.com:jslab/di-container.git"
  },
  "keywords": [
    "di",
    "ioc",
    "container",
    "dependency injection",
    "inversion of control"
  ],
  "author": "Anton Golub <a.golub@qiwi.com> (http://antongolub.com)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-flow": "^7.0.0-beta.40",
    "@babel/register": "^7.0.0-beta.40",
    "@std/esm": "^0.22.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "chai": "^4.1.2",
    "dirty-chai": "^2.0.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "flow-bin": "^0.66.0",
    "flow-coverage-report": "^0.4.1",
    "flow-remove-types": "^1.2.3",
    "glob": "^7.1.2",
    "install": "^0.10.4",
    "mocha": "^5.0.1",
    "npm": "^5.6.0",
    "nyc": "^11.4.1",
    "standard": "^11.0.0",
    "chai-spies": "^1.0.0",
    "chai-subset": "^1.6.0",
    "coveralls": "^3.0.0",
    "esdoc": "^1.0.4",
    "esdoc-flow-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "globals": [
      "it",
      "expect",
      "describe",
      "before",
      "after",
      "afterEach",
      "beforeEach"
    ],
    "rules": {
      "use-flow-type": 1
    },
    "ignore": [],
    "settings": {
      "flowtype": {
        "onlyFilesWithFlowAnnotation": true
      }
    }
  },
  "dependencies": {
    "esdoc-coverage-plugin": "^1.1.0"
  }
}
