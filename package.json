{
  "name": "@qiwi/di-container",
  "version": "1.0.2",
  "description": "Experiments around DI and IoC",
  "main": "dist/index.js",
  "@std/esm": {
    "mode": "all",
    "cjs": true
  },
  "scripts": {
    "flow": "flow",
    "flow_coverage": "flow-coverage-report -i 'src/**/*.js' -t html -t json -t text --threshold 90",
    "build": "babel src --out-dir dist",
    "lint": "standard src/**/*.js test/**/*.js",
    "mocha": "mocha --opts .mocha.opts report",
    "nyc": "nyc --reporter=lcov npm run mocha",
    "test": "npm run flow_coverage && npm run lint && npm run nyc",
    "doc": "esdoc",
    "test_with_report": "npm test && npm run codeclimate_push && npm run coveralls_push",
    "codeclimate_push": "codeclimate-test-reporter < ./coverage/lcov.info",
    "coveralls_push": "cat ./coverage/lcov.info | coveralls",
    "minor": "npm test && npm run build && standard-version --release-as minor && npm run postversion",
    "major": "npm test && npm run build && standard-version --release-as major && npm run postversion",
    "release": "npm test && npm run build && standard-version && npm run postversion",
    "postversion": "npm run push && npm publish",
    "push": "git push -f --follow-tags origin master && git push -f --follow-tags public master"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:qiwi/di-container.git"
  },
  "keywords": [
    "di",
    "ioc",
    "container",
    "dependency injection",
    "inversion of control"
  ],
  "author": "Anton Golub <a.golub@qiwi.com> (http://antongolub.com)",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-flow": "^7.0.0-beta.44",
    "@babel/register": "^7.0.0-beta.44",
    "@std/esm": "^0.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "chai": "^4.1.2",
    "chai-spies": "^1.0.0",
    "chai-subset": "^1.6.0",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.0",
    "dirty-chai": "^2.0.1",
    "esdoc": "^1.0.4",
    "esdoc-coverage-plugin": "^1.1.0",
    "esdoc-flow-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "flow-bin": "^0.69.0",
    "flow-coverage-report": "^0.5.0",
    "flow-remove-types": "^1.2.3",
    "glob": "^7.1.2",
    "install": "^0.11.0",
    "mocha": "^5.0.5",
    "npm": "^5.8.0",
    "nyc": "^11.6.0",
    "standard": "^11.0.1",
    "standard-version": "^4.3.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "globals": [
      "it",
      "expect",
      "describe",
      "before",
      "after",
      "afterEach",
      "beforeEach"
    ],
    "rules": {
      "use-flow-type": 1
    },
    "ignore": [],
    "settings": {
      "flowtype": {
        "onlyFilesWithFlowAnnotation": true
      }
    }
  }
}
