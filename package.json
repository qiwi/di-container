{
  "name": "@qiwi/di-container",
  "version": "1.0.2",
  "description": "Experiments around DI and IoC",
  "main": "dist/index.js",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "dist",
    "typings"
  ],
  "esm": {
    "mode": "all",
    "cjs": true
  },
  "scripts": {
    "flow": "flow",
    "flow_coverage": "flow-coverage-report -i 'src/**/*.js' -t html -t json -t text --threshold 90",
    "build": "babel src --out-dir dist",
    "lint": "standard src/**/*.js test/**/*.js --fix",
    "mocha": "mocha --opts .mocha.opts report",
    "nyc": "nyc --reporter=lcov npm run mocha",
    "test": "npm run flow_coverage && npm run lint && npm run nyc",
    "doc": "esdoc",
    "test_with_report": "npm test && npm run coveralls_push",
    "coveralls_push": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:qiwi/di-container.git"
  },
  "keywords": [
    "di",
    "ioc",
    "container",
    "dependency injection",
    "inversion of control"
  ],
  "author": "Anton Golub <a.golub@qiwi.com> (http://antongolub.com)",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.2.10",
    "@semantic-release/npm": "^5.1.4",
    "semantic-release": "^15.13.3",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "esm": "^3.2.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "chai-subset": "^1.6.0",
    "coveralls": "^3.0.2",
    "dirty-chai": "^2.0.1",
    "esdoc": "^1.1.0",
    "esdoc-coverage-plugin": "^1.1.0",
    "esdoc-flow-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "flow-bin": "^0.92.0",
    "flow-coverage-report": "^0.6.1",
    "flow-remove-types": "^1.2.3",
    "glob": "^7.1.3",
    "mocha": "^5.2.0",
    "nyc": "^15.0.0",
    "standard": "^12.0.1"
  },
  "release": {
    "branch": "master",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "globals": [
      "it",
      "expect",
      "describe",
      "before",
      "after",
      "afterEach",
      "beforeEach"
    ],
    "rules": {
      "use-flow-type": 1
    },
    "ignore": [],
    "settings": {
      "flowtype": {
        "onlyFilesWithFlowAnnotation": true
      }
    }
  }
}
